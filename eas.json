
{
  "cli": {
    "version": ">= 7.8.2",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release",
        "distribution": "store",
        "autoIncrement": "buildNumber"
      },
      "env": {
        "APPLE_API_KEY_BASE64": "$APPLE_API_KEY_BASE64",
        "APPLE_API_KEY_ID": "$APPLE_API_KEY_ID",
        "APPLE_API_KEY_ISSUER_ID": "$APPLE_API_KEY_ISSUER_ID"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "hiroakiya1981@gmail.com",
        "ascAppId": "6738971619",
        "appleTeamId": "8BXN9CYUCD"
      }
    }
  }
}
